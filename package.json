{
  "name": "komplete-kontrol-cli",
  "version": "1.0.0",
  "description": "Ultimate AI coding assistant integrating Roo Code, /auto, and advanced autonomous features",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "komplete": "dist/index.js"
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "start": "bun run dist/index.js",
    "test": "bun test",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "coding-assistant",
    "autonomous",
    "cli",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "ora": "^8.0.1",
    "zod": "^3.22.4"
  }
}
